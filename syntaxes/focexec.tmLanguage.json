{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "focexec",
	"scopeName": "source.fex",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#variables"
		}
	],
	"repository": {
		"comments": {
			"match": "-\\*([^\\n]*)",
			"name": "comment.line.webfocus"
		},
		"strings": {
			"match": "'([^']*)'",
			"name": "string.quoted.single.webfocus"
		},
		"keywords": {
			"match": "\\b(?:DEFINE|TABLE|COLUMN|SUM|BY)\\b",
			"name": "keyword.control.webfocus"
		},
		"operators": {
			"match": "\\+|-|\\*|/|=",
			"name": "keyword.operator.webfocus"
		},
		"numbers": {
			"match": "\\b\\d+\\b",
			"name": "constant.numeric.webfocus"
		},
		"variables": {
			"match": "\\$[a-zA-Z_][a-zA-Z0-9_]*",
			"name": "variable.webfocus"
		}
	}
}
{
  "name": "webfocus",
  "displayName": "WebFOCUS",
  "description": "WebFOCUS Language Support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "focexec",
        "aliases": [
          "WebFOCUS",
          "FOCUS Executable",
          "Procedure"
        ],
        "extensions": [
          ".fex"
        ],
        "configuration": "./languages/focexec.json"
      }
    ],
    "grammars": [
      {
        "language": "focexec",
        "scopeName": "source.fex",
        "path": "./syntaxes/focexec.tmLanguage.json"
      }
    ]
  }
}